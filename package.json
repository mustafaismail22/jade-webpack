{
  "name": "jade-webpack",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && npm run watch",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --colors --display-error-details --display-reasons --config webpack.config.js",
    "build": "npm run clean && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.config.js",
    "clean": "rimraf build"
  },
  "author": "Mustafa ismail",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap": "^3.3.6",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.4",
    "extract-loader": "^0.1.0",
    "file-loader": "^0.10.1",
    "glob": "^7.1.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "pug": "^2.0.0-beta11",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "webpack": "^2.2.1"
  },
  "dependencies": {}
}
